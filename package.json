{
  "name": "escomplex-plugin-formats-xml",
  "version": "0.0.1",
  "homepage": "https://github.com/typhonjs-node-escomplex/escomplex-plugin-formats-xml/",
  "description": "Provides a plugin for typhonjs-escomplex project result / module report transformation to XML.",
  "license": "MPL-2.0",
  "author": {
    "name": "Mike Leahy"
  },
  "main": "./dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/typhonjs-node-escomplex/escomplex-plugin-formats-xml.git"
  },
  "maintainers": [
    {
      "name": "Mike Leahy",
      "email": "support@typhonjs.io",
      "url": "https://github.com/typhonrt"
    }
  ],
  "dependencies": {
    "js2xmlparser": "^1.0.0",
    "typhonjs-escomplex-commons": "^0.0.11"
  },
  "devDependencies": {
    "typhonjs-config-eslint": "^0.4.0",
    "typhonjs-escomplex-test-data": "git+https://git@github.com/typhonjs-node-escomplex/typhonjs-escomplex-test-data.git",
    "typhonjs-npm-build-test": "^0.3.0"
  },
  "scripts": {
    "build": "babel-node ./node_modules/typhonjs-npm-scripts-build-babel/scripts/build.js",
    "build-syntax": "babel ./node_modules/typhonjs-escomplex-commons/src -d ./node_modules/typhonjs-escomplex-commons/dist",
    "esdoc": "esdoc -c .esdocrc",
    "eslint": "eslint .",
    "prepublish": "babel-node ./node_modules/typhonjs-npm-scripts-publish/scripts/prepublish.js",
    "test": "babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha.js",
    "test-coverage": "babel-node ./node_modules/typhonjs-npm-scripts-test-mocha/scripts/mocha-istanbul.js"
  },
  "keywords": [
    "typhonjs",
    "escomplex",
    "plugin",
    "format",
